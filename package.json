{
  "name": "@mocks-server/admin-api-paths",
  "version": "1.0.7",
  "description": "Api paths of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-paths",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint *.js test jest.config.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:coverage": "npm run test",
    "test:ci": "npm run test"
  },
  "devDependencies": {
    "@babel/preset-env": "7.11.0",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "13.0.2",
    "@rollup/plugin-node-resolve": "8.4.0",
    "babel-jest": "26.2.2",
    "coveralls": "3.0.9",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "rollup": "2.23.0",
    "rollup-plugin-terser": "6.1.0"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "*.js": "eslint",
    "jest.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 13.x || 14.x"
  }
}
